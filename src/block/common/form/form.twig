{% if view == "signin" %}
	<form action="#" class="{{prefix}}panel  {{mod}}">
		{% include "../page-header/tpl.twig" with {
			prefix:"page-header__", 
			mod:heading_mod,
			level:heading_level, 
			heading:heading
		} %}

		<div class="{{prefix}}row  grid__row  {{mod}}">
			{% include "elem-tpl.twig" with {view: "input", prefix:prefix, mod:mod, color:color, list: [
				{
					"type":"tel", 
					"required":"required", 
					"placeholder":"Номер телефона",
					"group":""
				},
				{
					"type":"email", 
					"required":"required", 
					"placeholder":"E-mail",
					"group":""
				}
			]} %}
			<div class="{{prefix}}btn  {{mod}}">
				{% include "../button/btn.twig" with {
					view:"btn",
					prefix:"btn__", 
					type:'submit', 
					arrow: "true",
					icon: "arrow-next",
					group:content_mod, 
					onsubmit:'return false', 
					location:page.index, 
					mod:"is--success  is--arrow-next", 
					name:"Войти в личный кабинет" 
				} %}
			</div>
			<div class="{{prefix}}btn  {{mod}}  is--text">
				Или вы можете <a href="{{page.signup}}">зарегистрироваться</a>
			</div>
		</div>
	</form>
{% endif %}

