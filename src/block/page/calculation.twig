					{% include "../common/card/card.twig" with {
					 	view: "calc_card", 
					 	prefix:prefix.card, 
					 	mod:"is--calc-card", 
					 	list:list
					} %}
				</div>
				<div class="{{content_prefix}}link  {{content_mod}}">				
					{% include "../common/button/btn.twig" with {
						view:"link",
						prefix:"link__", 
						arrow: "true",
						icon: "arrow-next",
						mod:"is--arrow  is--success", 
						link:page.calculation, 
						name:"Все расчеты" 
					} %}
				</div>
			{% else %}
				<div class="{{content_prefix}}row  {{content_mod}}" data-note="{{heading_not}}">
